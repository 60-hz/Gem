GEM_TARGETS = plugins Base @GEM_TARGETS@
EXT = @EXT@

GEM_DEFINES=@DEFINES@ @DEFS@ -DGEM_INTERNAL -DHAVE_VERSION_H
GEM_CXXFLAGS= @X_CFLAGS@ @PKG_CFLAGS@ -I.. @INCLUDES@ $(GEM_DEFINES) @CXXFLAGS@ @GEM_RTE_CFLAGS@ $(CXXFLAGS)
GEM_LDFLAGS = @LDFLAGS@ $(LDFLAGS)
GEM_LIBS = @X_LIBS@ @PKG_LIBS@ @LIBS@ @GEM_RTE_LIBS@ $(LIBS)

GEM_ARCH_CXXFLAGS = @ARCH_FLAGS@
GEM_ARCH_LDFLAGS = @ARCH_FLAGS@

MAKEDEP_FLAGS = @MAKEDEP_FLAGS@

CXXCPP=@CXXCPP@
CXX=@CXX@

STRIP = @STRIP@
STRIP_UNNEEDED ?= @STRIP_UNNEEDED@

DOS2UNIX = @DOS2UNIX@

INSTALL = @INSTALL@

prefix = @prefix@
exec_prefix = @exec_prefix@
libdir = @libdir@
datadir = @datarootdir@
GEMLIBDIR = $(libdir)
