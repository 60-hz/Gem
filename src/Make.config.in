GEM_LDFLAGS = @LDFLAGS@ $(LDFLAGS)
GEM_LIBS = @X_LIBS@ @PKG_LIBS@ @LIBS@ @GEM_RTE_LIBS@ $(LIBS)

GEM_ARCH_CXXFLAGS = @ARCH_FLAGS@
GEM_ARCH_LDFLAGS = @ARCH_FLAGS@

MAKEDEP_FLAGS = @MAKEDEP_FLAGS@

STRIP_UNNEEDED ?= @STRIP_UNNEEDED@
STRIP ?= @STRIP@
DOS2UNIX = @DOS2UNIX@

PDLIBDIR = @prefix@/lib/pd
PDREFPATH = @REFERENCEPATH@

INSTALL = @INSTALL@

CONFIGUREFLAGS ?= @CONFIGUREFLAGS@
